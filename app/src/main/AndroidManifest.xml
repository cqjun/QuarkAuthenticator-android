<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.quarkblockchain.czgoogleauth">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppContext"
        android:allowBackup="true"
        android:icon="@drawable/yanzhengqi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/yanzhengqi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="cn.quarkblockchain.czgoogleauth.AuthenticatorActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnterKeyActivity"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" />

        <activity android:name=".AddAccountActivity" />
        <activity android:name=".CustomScanActivity" />
        <activity android:name=".HelpActivity"/>
        <activity android:name=".PrivacyActivity"/>
    </application>

</manifest>